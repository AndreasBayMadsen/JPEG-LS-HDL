{
    "configurations": [
        {
            "name": "C/C++: g++ build and debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": ["-nomarkers", "-noruns", "-rows", "240", "-columns", "320", "-bits", "5", "-T1", "3", "-T2", "7", "-T3", "21", "-input-file", "${fileDirname}/../test_image.pgm", "-input-endian", "big", "-output-file", "${fileDirname}/../test_image_unmarked.jls"],                            // To compress local test image
            // "args": ["-d", "-nomarkers", "-noruns", "-rows", "768", "-columns", "512", "-bits", "5", "-T1", "3", "-T2", "7", "-T3", "21", "-input-file", "${fileDirname}/../test_image_unmarked.jls", "-output-file", "${fileDirname}/../test_image_unmarked_outp.pgm", "-output-endian", "big"],    // To decompress local test image
            // "args": ["-d", "-nomarkers", "-noruns", "-rows", "512", "-columns", "768", "-bits", "5", "-T1", "3", "-T2", "7", "-T3", "21", "-input-file", "/home/andreas/Desktop/ProfileProject/JPEG-LS-HDL/kodak_dataset/sim_output_red.jls", "-output-file", "/home/andreas/Desktop/ProfileProject/JPEG-LS-HDL/kodak_dataset/sim_output_red.pgm", "-output-endian", "big"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: g++ build active file",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ],
    "version": "2.0.0"
}